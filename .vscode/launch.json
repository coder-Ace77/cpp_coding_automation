{
    "version": "0.2.0",
    "configurations": [
      
      {
        "name": "Debug Competitive Code",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/main.cpp",
        "args": ["<", "${workspaceFolder}/input.txt", ">", "${workspaceFolder}/output.txt"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "preLaunchTask": "Build",
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb"
      }
    ]
  }
  